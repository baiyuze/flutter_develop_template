  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter web",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["-d", "chrome", "--web-port", "8000"]
    },
    {
      "name": "Flutter macOS",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["-d", "macOS"]
    },
    {
      "name": "Flutter apk",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["-d", "自定义设备id"]
    }
  ]
}
